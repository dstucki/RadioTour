<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >

    <TextView
        android:id="@+id/getStartNr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="4px"
        android:layout_marginRight="10px"
        android:layout_marginTop="4px"
        android:text="Start-Nr."
        android:textSize="20px" >
    </TextView>
    <TextView
        android:id="@+id/getCountry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="4px"
        android:layout_marginRight="10px"
        android:layout_marginTop="4px"
        android:text="getCountry"
        android:textSize="20px" >
    </TextView>

    <TextView
        android:id="@+id/getTeam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          android:layout_marginLeft="4px"
        android:layout_marginRight="10px"
        android:layout_marginTop="4px"
        android:text="getTeam"
        android:textSize="20px" >
    </TextView>

    <TextView
        android:id="@+id/getName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          android:layout_marginLeft="4px"
        android:layout_marginRight="10px"
        android:layout_marginTop="4px"
        android:text="getName"
        android:textSize="20px" >
    </TextView>

    <TextView
        android:id="@+id/getBirthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
          android:layout_marginLeft="4px"
        android:layout_marginRight="10px"
        android:layout_marginTop="4px"
        android:text="getBirthday"
        android:textSize="20px" >
    </TextView>

</LinearLayout>